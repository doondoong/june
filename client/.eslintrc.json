{
  "parser": "babel-eslint",
  "plugins": [
    "react", "react-hooks", "prettier"
  ],
  "extends": [
    // "eslint:recommended",
    // "plugin:prettier/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "env": {
    "browser": false,
    "node": true
  },
  "ignorePatterns": [
    "node_modules/"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}